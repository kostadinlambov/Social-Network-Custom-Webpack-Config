{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "ie": "11"
                },
                "useBuiltIns": "usage",
                "corejs": 3,
                "debug": true
            }
        ],
        [
            "@babel/preset-react",
            {
                 "targets": {
                    "ie": "11"
                },
                "useBuiltIns": true,
                "debug": true
            }
        ]
    ],

    "plugins" : [
        "@babel/plugin-proposal-class-properties"
    ]
}